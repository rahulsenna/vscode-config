    "editor.semanticTokenColorCustomizations": {
        "[Default Dark+]": {

            "rules": {
                "variable": "#94dbfd",
                "property": "#DADADA",

                "macro": "#beb7ff",
                "variable.readonly": "#beb7ff",
                "builtinConstant": {"foreground":"#beb7ff", "fontStyle": "italic"},

                "method": "#dbdbaa",
                "function": "#dbdbaa",
                "function.builtin": {"fontStyle": "italic bold"},
                "magicFunction": {"foreground": "#af5070", "fontStyle": "bold italic"},

                "module": "#f9ae58",
                "namespace":{"foreground":"#4ec9b0", "fontStyle":"italic"} ,                
                "class": "#4eaab0",
                "class.builtin": {"fontStyle": "italic"},
                "type": "#4ec9b0",
                "interface": {"foreground":"#4eaab0", "fontStyle": "italic"},

                "enum": "#4eaacf",
                "enumMember": {"foreground": "#DADADA", "fontStyle": "italic"},
                "operator": "#C586C0",
                
                "parameter": "#C586C0",
                "selfParameter": {"foreground":"#C586C0", "fontStyle": "italic bold"},
                "variable.static": "#66bde6",
                "function.static": "#c9c97f",
                "method.static": "#c9c97f",
                "decorator": "#ff0000",
                "*.static": "#ff0000",

            },  
        },
        "[Gruvbox Dark Hard]": {
            "rules": {
                // "variable": "#83a598",
                "variable": "#8ec07c",
                // "variable": "#458588",
                "property": "#ebdbb2",

                "macro": "#ae7cc0",
                "variable.readonly": "#ae7cc0",
                "builtinConstant": {"foreground":"#ae7cc0", "fontStyle": "italic"},
                // "function": "#fe8019",
                // "method": "#d08019",
                // "method": "#d79921",
                "method": "#fad02f",
                "function": "#fabd2f",
                "function.builtin": {"fontStyle": "italic bold"},
                "magicFunction": {"foreground": "#2161d7", "fontStyle": "bold italic"},


                "module": "#fe8019",
                "namespace":{"foreground":"#fe8019", "fontStyle":"italic"} ,
                "class": "#b8CC26",
                "class.builtin": {"fontStyle": "italic"},
                "type": "#b8bb26",
                "interface": {"foreground":"#98BD1a", "fontStyle": "italic"},
                
                "enum": "#f8f8f2",
                "enumMember": {"foreground": "#ebdbb2", "fontStyle": "italic"},
                "operator": "#b16286",

                "parameter": "#d3869b",
                "selfParameter": {"foreground":"#d3869b","fontStyle": "italic bold"},
            },
        },
    },
    

