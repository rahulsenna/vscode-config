    "editor.semanticTokenColorCustomizations": {
        "[Default Dark+]": {

            "rules": {
                "variable": "#94dbfd",
                "property": "#DADADA",

                "macro": "#beb7ff",
                "variable.readonly": "#beb7ff",
                "builtinConstant": {"foreground":"#beb7ff", "fontStyle": "italic"},

                "method": "#dbdbaa",
                "function": "#dbdbaa",
                "function.builtin": {"fontStyle": "italic bold"},
                "magicFunction": {"foreground": "#af5070", "fontStyle": "bold italic"},

                "module": "#f9ae58",
                "namespace":{"foreground":"#4ec9b0", "fontStyle":"italic"} ,                
                "class": "#4eaab0",
                "class.builtin": {"fontStyle": "italic"},
                "type": "#4ec9b0",
                "interface": {"foreground":"#4eaab0", "fontStyle": "italic"},

                "enum": "#4eaacf",
                "enumMember": {"foreground": "#DADADA", "fontStyle": "italic"},
                "operator": "#C586C0",
                
                "parameter": "#C586C0",
                "selfParameter": {"foreground":"#C586C0", "fontStyle": "italic bold"},
                "variable.static": "#66bde6",
                "function.static": "#c9c97f",
                "method.static": "#c9c97f",
                "decorator": "#ff0000",
                "*.static": "#ff0000",

            },  
        },
        "[Catppuccin Mocha]": {

            /*

            Color	       Hex	     RGB	HSL
            Rosewater      #f5e0dc	 rgb(245, 224, 220)	 hsl(10deg, 56%, 91%)         
                        
            Flamingo       #f2cdcd	 rgb(242, 205, 205)	 hsl(0deg, 59%, 88%)        
                        
            Pink           #f5c2e7	 rgb(245, 194, 231)	 hsl(316deg, 72%, 86%)    
                        
            Mauve          #cba6f7	 rgb(203, 166, 247)	 hsl(267deg, 84%, 81%)     
                        
            Red            #f38ba8	 rgb(243, 139, 168)	 hsl(343deg, 81%, 75%)   
                        
            Maroon         #eba0ac	 rgb(235, 160, 172)	 hsl(350deg, 65%, 77%)      
                        
            Peach          #fab387	 rgb(250, 179, 135)	 hsl(23deg, 92%, 75%)     
                        
            Yellow         #f9e2af	 rgb(249, 226, 175)	 hsl(41deg, 86%, 83%)      
                        
            Green          #a6e3a1	 rgb(166, 227, 161)	 hsl(115deg, 54%, 76%)     
                        
            Teal           #94e2d5	 rgb(148, 226, 213)	 hsl(170deg, 57%, 73%)    
                        
            Sky            #89dceb	 rgb(137, 220, 235)	 hsl(189deg, 71%, 73%)   
                        
            Sapphire       #74c7ec	 rgb(116, 199, 236)	 hsl(199deg, 76%, 69%)        
                        
            Blue           #89b4fa	 rgb(137, 180, 250)	 hsl(217deg, 92%, 76%)    
                        
            Lavender       #b4befe	 rgb(180, 190, 254)	 hsl(232deg, 97%, 85%)        
                        
            Text           #cdd6f4	 rgb(205, 214, 244)	 hsl(226deg, 64%, 88%)    
                        
            Subtext 1      #bac2de	 rgb(186, 194, 222)	 hsl(227deg, 35%, 80%)         
                        
            Subtext 0      #a6adc8	 rgb(166, 173, 200)	 hsl(228deg, 24%, 72%)         
                        
            Overlay 2      #9399b2	 rgb(147, 153, 178)	 hsl(228deg, 17%, 64%)         
                        
            Overlay 1      #7f849c	 rgb(127, 132, 156)	 hsl(230deg, 13%, 55%)         
                        
            Overlay 0      #6c7086	 rgb(108, 112, 134)	 hsl(231deg, 11%, 47%)         
                        
            Surface 2      #585b70	 rgb(88, 91, 112)	 hsl(233deg, 12%, 39%)         
                        
            Surface 1      #45475a	 rgb(69, 71, 90)	 hsl(234deg, 13%, 31%)         
                        
            Surface 0      #313244	 rgb(49, 50, 68)	 hsl(237deg, 16%, 23%)         
                        
            Base           #1e1e2e	 rgb(30, 30, 46)	 hsl(240deg, 21%, 15%)    
                        
            Mantle         #181825	 rgb(24, 24, 37)	 hsl(240deg, 21%, 12%)      
                        
            Crust          #11111b	     
                        
            */

            "rules": {
                // "variable": "#DADADA", // Sky
                // "variable": "#f9e2af", // Yellow
                "variable": "#89b4fa", // blue
                "property": "#DADADA", // Base

                "macro": "#f5c2e7",    // Lavender
                "variable.readonly": "#b4befe", // Lavender
                "builtinConstant": {"foreground":"#beb7ff", "fontStyle": "italic"}, // Lavender

                // "method": "#89b4fa",   // Yellow
                // "function": "#89b4fa", // Yellow
                "method": "#f9e2af",   // Yellow
                "function": "#f9e2af", // Yellow
                "function.builtin": {"fontStyle": "italic bold"},
                "magicFunction": {"foreground": "#f28fad", "fontStyle": "bold italic"}, // Pink

                "module": "#fab387",    // Peach
                "namespace":{"foreground":"#94e2d5", "fontStyle":"italic"} , // Cyan
                "class": "#f9e2af",     // Green
                "class.builtin": {"fontStyle": "italic"},
                "type": "#94e2d5",      // Cyan
                "interface": {"foreground":"#f9e2af", "fontStyle": "italic"}, // Green

                "enum": "#4eaacf",      // Blue
                "enumMember": {"foreground": "#DADADA", "fontStyle": "italic"}, // Base
                "operator": "#C586C0",    // Mauve

                "parameter": "#f38ba8", //  Red
                "selfParameter": {"foreground":"#C586C0", "fontStyle": "italic bold"}, // Mauve
                "variable.static": "#66bde6", // Blue
                "function.static": "#c9c97f", // Green-Yellow
                "method.static": "#c9c97f",   // Green-Yellow
                "decorator": "#f0c674",     // Gold (more muted than red)
                "*.static": "#f0c674",      // Gold (more muted than red)
            },  
        },
        "[Gruvbox Dark Hard]": {
            "rules": {
                // "variable": "#83a598",
                "variable": "#8ec07c",
                // "variable": "#458588",
                "property": "#ebdbb2",

                "macro": "#ae7cc0",
                "variable.readonly": "#ae7cc0",
                "builtinConstant": {"foreground":"#ae7cc0", "fontStyle": "italic"},
                // "function": "#fe8019",
                // "method": "#d08019",
                // "method": "#d79921",
                "method": "#fad02f",
                "function": "#fabd2f",
                "function.builtin": {"fontStyle": "italic bold"},
                "magicFunction": {"foreground": "#2161d7", "fontStyle": "bold italic"},


                "module": "#fe8019",
                "namespace":{"foreground":"#fe8019", "fontStyle":"italic"} ,
                "class": "#b8CC26",
                "class.builtin": {"fontStyle": "italic"},
                "type": "#b8bb26",
                "interface": {"foreground":"#98BD1a", "fontStyle": "italic"},
                
                "enum": "#f8f8f2",
                "enumMember": {"foreground": "#ebdbb2", "fontStyle": "italic"},
                "operator": "#b16286",

                "parameter": "#d3869b",
                "selfParameter": {"foreground":"#d3869b","fontStyle": "italic bold"},
            },
        },
    },
    

