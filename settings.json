{
    "editor.tokenColorCustomizations": {
        "textMateRules":
        [
            {
                "scope": "variable.other.enummember",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "storage.type.function",
                "settings": {
                    "fontStyle": "bold italic",
                }
            },
            {
                "scope": "storage.modifier",
                "settings": {
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": "keyword.operator.new",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "storage.type.function",
                "settings": {
                    "fontStyle": "bold italic"

                }
            },

        ],

        "[Default Dark+]": {

            // "comments": "#707070",
            "comments": "#98fb98",
            "strings": "#cd9069",
            "numbers": "#6897BB",
            "textMateRules": [
                {
                    "scope": "keyword.control.directive",
                    "settings": {
                        "foreground": "#9b9b9b",
                    }
                },

                {
                    "scope": "variable.other.property",
                    "settings": {
                        "foreground": "#DADADA",
                    }
                },
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#569CD6",
                    }
                },
            ],

        },

        "[Gruvbox Dark Hard]": {
            "textMateRules": [
                {
                    "scope": "support.type",
                    "settings": {
                        "foreground": "#b8bb26",
                    }
                },
                {
                    "scope": "keyword.operator.new",
                    "settings": {
                        "foreground": "#FB4934",
                    }
                },
                {
                    "scope": "storage.type.function",
                    "settings": {
                        "foreground": "#FB4934",

                    }
                },
                {
                    "scope": "constant.numeric",
                    "settings": {
                        "foreground": "#ebdbb2",
                    }
                }
                
            ],

        }
    },
