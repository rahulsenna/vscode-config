[

    //--------------insert modes  --------------

    {
        "key": "a k",
        "command": "rahulvscodeplugin.command_and_insert", 
        "args": "editor.action.insertLineAfter",
        "when": "editorTextFocus && modal.normal && !terminalFocus" 
    },
    {
        "key": "a i",
        "command": "rahulvscodeplugin.command_and_insert",
        "args": "editor.action.insertLineBefore",
        "when": "editorTextFocus && modal.normal && !terminalFocus"
    },
    {
        "key": "a j",
        "command": "rahulvscodeplugin.command_and_insert",
        "args": "cursorHome",
        "when": "editorTextFocus && modal.normal && !terminalFocus" 
    },
    {
        "key": "a l",
        "command": "rahulvscodeplugin.command_and_insert",
        "args": "cursorEnd",
        "when": "editorTextFocus && modal.normal && !terminalFocus"
    },
    {
