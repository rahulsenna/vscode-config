	"combination": {
		"prefix": [
			"comb"
		],
		"body": [
			"struct Combination",
			"{",
			"\tCombination(std::vector<int> &arr, int len) : elements(arr), current_combination(len), current_indices(len), current_length(len), first(true)",
			"\t{",
			"\t\tfor (int i = 0; i < current_length; ++i)",
			"\t\t{",
			"\t\t\tcurrent_indices[i] = i;",
			"\t\t\tcurrent_combination[i] = elements[i];",
			"\t\t}",
			"\t}",
			"\tbool has_next()",
			"\t{",
			"\t\treturn current_indices[0] < elements.size() - current_length || first;",
			"\t}",
			"\tstd::vector<int> &next()",
			"\t{",
			"\t\tif (first)",
			"\t\t{",
			"\t\t\tfirst = false;",
			"\t\t\treturn current_combination;",
			"\t\t}",
			"\t\t// Find the rightmost index that can be incremented",
			"\t\tint i = current_length - 1;",
			"\t\twhile (i >= 0 && current_indices[i] == elements.size() - current_length + i)",
			"\t\t\t--i;",
			"\t\tif (i >= 0)",
			"\t\t{",
			"\t\t\t++current_indices[i];",
			"\t\t\tcurrent_combination[i] = elements[current_indices[i]];",
			"\t\t\t// Update subsequent indices and current_combination",
			"\t\t\tfor (int j = i + 1; j < current_length; ++j)",
			"\t\t\t{",
			"\t\t\t\tcurrent_indices[j] = current_indices[j - 1] + 1;",
			"\t\t\t\tcurrent_combination[j] = elements[current_indices[j]];",
			"\t\t\t}",
			"\t\t}",
			"\t\treturn current_combination;",
			"\t}",
			"",
			"private:",
			"\tstd::vector<int> &elements;",
			"\tstd::vector<int> current_combination;",
			"\tstd::vector<int> current_indices;",
			"\tint current_length;",
			"\tbool first;",
			"};",
			"\n\n",
			"//std::vector<int> numbers = {1, 2, 3, 4, 5};",
			"//Combination combinations(numbers, 3);",
			"//while (combinations.has_next())",
			"//{",
			"//  auto &combination = combinations.next();",
			"//  // DO SOMETHING WITH `combination `",
			"//} ",
		],
		"description": "Code for Combinations"
